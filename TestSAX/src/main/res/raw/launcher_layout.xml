	<!-- Launcher公共定义 -->
	<PageCommon>
		<!--页面背景图，每个页面可以独立设置不同背景-->
		<DefaultBackground><![CDATA[720p/assets/images/bg_img.png]]></DefaultBackground>
		<!-- 注：Launcher界面下方的页面导航条，不需要特别定义，自动根据Page列表和id生成列表和当前高亮 -->
		<!-- 文本式导航栏中文本标签高亮选中时的背景图片 -->
		<FocusNavBgImg><![CDATA[720p/assets/images/navmenu_onbg.png]]></FocusNavBgImg>
	</PageCommon>
	<!-- MaxPageNumber：配置的最大栏目页数，从0开始，如配置栏目页数超出该值，取前MaxPageNumber个PageNode展示 -->
	<PageList MaxPageNumber="3">
		<!-- id：页面唯一标识，不允许两个页面出现相同ID。可用于作为定义页面跳转时的标识；
			 order：页面排序页码，从0开始，必填，数字越小就排在越前；
			 PageName：页面名称；  -->
		<Page id="myhome" order="0" width="5" height="3">
			<PageName><![CDATA[我的家庭]]></PageName>
			<!-- 页面自定义的背景 -->
			<PageBackground><![CDATA[720p/assets/images/bg_img.png]]></PageBackground>
